pipeline{
    agent any
    stages{
        stage("working with loops"){
            script{
                for (sub in["aws","azure","devops","git"]){
                    println "I learned subjects like ${sub}"
                }
                for (i=1;i<=5,i++){
                    println "my i value is ${i}"
                }
                j=10
                while(j<=20){
                    println "my j value is ${j}"
                }
            }
        }
    }
}
